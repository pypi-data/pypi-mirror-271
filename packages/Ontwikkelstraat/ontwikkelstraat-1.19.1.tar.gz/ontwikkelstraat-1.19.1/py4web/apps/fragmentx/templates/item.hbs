 <div class="container is-fullhd"><div class="columns m-0 has-background-white" style="padding-bottom: 10rem;">

    <div class="column is-12 is-8-widescreen p-6">
        <article class="p-6" id="{{item.id}}">
            <div class="is-relative mb-6">
                <figure class="edwh-placeholder image is-fullwidth">
                    {{#if item.thumbnail.uri}}
                        <img src="{{item.thumbnail.uri}}" alt="Niet noodzakelijke weergave.">
                    {{else}}
                        <img src="https://f003.backblazeb2.com/file/nl-meteddie-delen-permalinkable/image_placeholder.jpg"
                             alt="Standaard weergave van een item.">
                    {{/if}}

                </figure>
                <div class="edwh-stickers" style="position: absolute;">
                    {{#each item.stickers}}
                        {{#if svg}}
                            <div style="cursor: pointer;" title="{{sticker_tag.name}}"
                                 class="is-pulled-right"
                                 _="install Sticker(id: '{{sticker_tag.id}}')"
                            >
                                <!-- object kan geen onclick, dus vandaar een div + a er om heen -->
                                <div title="{{sticker_tag.name}}" style="display: block; z-index: 1;">
                                    <object id="svg-{{id}}" data="{{uri}}"
                                            class="edwh-sticker"
                                            style="z-index: -1; pointer-events: none;"
                                            type="image/svg+xml">
                                    </object>
                                </div>
                            </div>
                        {{else}}
                            <img
                                    style="cursor: pointer;"
                                    _="install Sticker(id:'{{sticker_tag.id}}')"

                                    id="{{id}}"
                                    src="{{uri}}"
                                    class="edwh-sticker"
                                    alt="{{sticker_tag.name}}" title="{{sticker_tag.name}}">
                        {{/if}}
                    {{/each}}
                </div>
            </div>
        <div class="columns m-0 pt-5 is-flex-direction-column">
            <form class="edwh-item-icon is-flex is-justify-content-space-between is-align-items-center mb-1"
                {{#if anonymous}}
                  _="install Auth"
                {{else}}
                  hx-trigger="ew:submit throttle:500ms"
                  hx-post="like"
                  hx-target="find .edwh-like-feedback"
                  _="install LikeButton(item_id: '{{item.id}}')"
                {{/if}}
            >
                <input type="hidden" value="{{item.id}}" name="subject"/>
                <input type="hidden" name="toggle"/>
                <input type="hidden" value="THUMBS" name="action"/>
                <h1 class="title is-size-3 p-0 pr-3 m-0">{{item.name}}</h1>
                <a
                    {{#if (has_my_thumb item)}}
                    class="is-flex thumbs-UP"
                    {{else}}
                    class="is-flex"
                    {{/if}}>
                    <svg viewBox="0 0 17.806 15.713"
                         class="edwh-icon">
                        <path
                                d="M16.383 1.423a4.784 4.784 0 00-6.81 0l-.67.67-.67-.67a4.815 4.815 0 00-6.81 6.81l7.48 7.48 7.48-7.48a4.784 4.784 0 000-6.81"
                                fill="#25303D" fill-rule="evenodd" data-name="Symbol 66"></path>
                    </svg>
                    <span class="ml-2 edwh-like-count" data-gid="{{item.id}}">
                                ({{thumb_count item}})
                            </span>
                    <span class="edwh-like-feedback"></span>
                </a>
            </form>
            <div class="subtitle is-flex">{{#if item.time_to_read}}
                {{item.time_to_read}} min |
            {{else}}
                < 1 min |
            {{/if}} Laatste bewerking: {{item.ts_changed_hf}} <span class="mx-1">|</span>
                <span class="icon mx-1">
                         <object data="../../cmsx/static/images/cc-general.svg" height="100%" width="100%"></object>
                    </span>
                <span class="icon mx-1">
                         <object data="../../cmsx/static/images/cc-by.svg" height="100%" width="100%"></object>
                    </span>
                <span class="icon mx-1">
                         <object data="../../cmsx/static/images/cc-sa.svg" height="100%" width="100%"></object>
                    </span>
            </div>
            <div class="edwh-item-firstline my-5">{{item.firstline}}</div>
            <div class="content">{{item.body}}</div>
            {{#if item.attachments}}
                <div class="edwh-item-title has-text-orange">Documenten</div>
                <div class="columns is-multiline">
                    {{#each item.attachments}}
                        {{#if uri}}
                            <div class="column is-4">
                                <div class="box"
                                     style="background-color: rgba(255, 255, 255, 0); border-radius: 0.5em; box-shadow: none; color: #F4F7FC; display: block; padding: 1.25rem; border: 0.25em solid #ED7959;">
                                    <a class="columns is-mobile" href="{{uri}}" target="_blank"
                                       title="Download {{filename}}">
                                        <div class="column is-9">
                                            <div class="title is-size-6"
                                                 style="color: inherit;">{{filename}}</div>
                                        </div>
                                        <div class="column p-0 m-auto">
                                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"
                                                 class="m-auto is-block"
                                                 style="height: 2.5rem; width: 2.5rem;">
                                                <defs>
                                                    <style>.a {
                                                        fill: none;
                                                        stroke: currentColor;
                                                        stroke-linecap: round;
                                                        stroke-linejoin: round;
                                                        stroke-width: 1.5px;
                                                    }</style>
                                                </defs>
                                                <title>download-circle</title>
                                                <polyline class="a"
                                                          points="8.25 14.25 12 18 15.75 14.25"></polyline>
                                                <line class="a" x1="12" y1="6.75" x2="12"
                                                      y2="18"></line>
                                                <circle class="a" cx="12" cy="12" r="11.25"></circle>
                                            </svg>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        {{/if}}
                    {{/each}}
                </div>
            </div>
            {{/if}}
            {{#if item.backgrounds}}
                <div class="edwh-column-full">
                    <div class="edwh-item-title has-text-orange">Media</div>
                    <div class="edwh-item-media-columns">
                        {{#each item.backgrounds}}
                            {{#if uri}}
                                <div class="edwh-item-media-column">
                                    <figure class="edwh-placeholder">
                                        <img class="edwh-modaltoggle" src="{{uri}}" alt="{{filename}}"
                                             _="on click set #modal_image's src to my src then call modal(#openimage)"
                                        >
                                    </figure>
                                </div>
                            {{/if}}
                        {{/each}}
                    </div>
                </div>
            {{/if}}
        </article>
    </div>
