{
    "tag": "$title",
    "overwrite": true,
    "folder": {
        "folder_output": "slurm_output",
        "folder_delete": ["data_output"],
        "folder_create": ["data_output"]
    },
    "pragmas": {
        "time":  "4:00:00",
        "nodes":  "1",
        "ntasks-per-node": "2",
        "mem":  "8G"
    },
    "vars": {
        "PYTHONUNBUFFERED": "1",
        "VARWORLD": "$var"
    },
    "commands": [
        {
            "tag": "version",
            "executable": "python3",
            "arguments": ["--version"]
        },
        {
            "tag": "hello",
            "executable": "python3",
            "arguments": ["run_slurm.py", "hello", "hello world!"]
        },
        {
            "tag": "goodbye",
            "executable": "python3",
            "arguments": ["run_slurm.py", "goodbye", "goodbye world!"]
        }
    ]
}
